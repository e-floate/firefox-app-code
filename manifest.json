{
  "description": "Phisherman JavaScript segment",
  "manifest_version": 2,
  "name": "Phisherman-JS",
  "version": "1.0.2.2",
  
  "permissions": [
    "webRequest",
    "<all_urls>",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["hyp_grab.js"]
    }
  ]
}